package My;

public class Literalobjet 
{
	public static void main(String[] args)
	{
		// TODO Auto-generated method stub
		
		String a = "Akash";         // Literal Object
		System.out.println(a);
		
		String b = "Akash";         // Literal Object    
		System.out.println(b);
		
		a=a.concat(" Verma ");
		System.out.println(a);
	}

}
